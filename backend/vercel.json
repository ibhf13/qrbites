{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/uploads/(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/r/(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/api/(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/",
        "dest": "/api/index.js"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "functions": {
      "api/index.js": {
        "maxDuration": 30
      }
    }
  }