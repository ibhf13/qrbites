{
    "name": "qrbites",
    "version": "1.0.0",
    "description": "A web-based platform for restaurant menu digitization",
    "main": "src/server.js",
    "_moduleAliases": {
        "@src": "src",
        "@models": "src/models",
        "@controllers": "src/controllers",
        "@routes": "src/routes",
        "@middlewares": "src/middlewares",
        "@utils": "src/utils",
        "@config": "src/config",
        "@validations": "src/validations",
        "@tests": "tests",
        "@seeders": "tests/seeders",
        "@mocks": "tests/fixtures/mocks",
        "@services": "src/services"
    },
    "scripts": {
        "start": "node src/server.js",
        "dev": "nodemon src/server.js",
        "lint": "eslint .",
        "seed": "node tests/seedPlanner.js",
        "seed:dev": "cross-env NODE_ENV=development node tests/seedPlanner.js",
        "seed:test": "cross-env NODE_ENV=test node tests/seedPlanner.js",
        "seed:production": "node seedDev.js",
        "test": "jest --watchAll --verbose --detectOpenHandles",
        "test:ci": "jest --ci --coverage --detectOpenHandles",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand"
    },
    "dependencies": {
        "axios": "^1.9.0",
        "bcryptjs": "^3.0.2",
        "chalk": "^4.1.2",
        "cors": "^2.8.5",
        "dotenv": "^16.5.0",
        "express": "^5.1.0",
        "express-rate-limit": "^7.5.0",
        "form-data": "^4.0.2",
        "helmet": "^8.1.0",
        "joi": "^17.13.3",
        "jsonwebtoken": "^9.0.2",
        "module-alias": "^2.2.3",
        "mongoose": "^8.14.1",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.2",
        "node-cache": "^5.1.2",
        "qrcode": "^1.5.4",
        "uuid": "^11.1.0",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.25.1",
        "cross-env": "^7.0.3",
        "eslint": "^9.25.1",
        "express-validator": "^7.2.1",
        "globals": "^16.0.0",
        "jest": "^29.7.0",
        "mongodb-memory-server": "^10.1.4",
        "nodemon": "^3.1.10",
        "supertest": "^7.1.0"
    }
}